
  <!-- need to make viewer larger & centered in div -->
  <model-viewer 
    id="reveal"
    alt="<%= upload.description %>" 
    src="<%= (url_for(upload.file)) %>" 
    poster="<%= (url_for(upload.thumbnail)) %>" 
    seamless-poster
    shadow-intensity="1" 
    camera-controls enable-pan>
  </model-viewer>

