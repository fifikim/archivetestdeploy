<div class="container">
  <div class="mt-5 p-5 box-rounded border-fill bg-white">
    <h1 class="page-header">archive</h1>
    <%= render 'search/form' %>

    <div class="row d-flex justify-content-between my-5 mx-auto">
      <% @uploads.each do |upload| %>
        <div id="uploads" class="col-lg-4 col-md-8 col-sm-12 p-5">
          <%= render upload %>
        </div>
      <% end %>
    </div>
    
  </div>
</div>




